<!DOCTYPE html>
<html lang="en">

<head>
  <% include /partials/head %>
</head>

<body>

  <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
    <% include /partials/nav %>
  </nav>

  <main class="container-fluid mt-5" role="main">
    <!-- <div class="row"> -->
    <%for (var result = 0; result < photoData.length; result++){%>
    <% if (result % 3 == 0) { %>
    <div class="row mb-3">
      <% } %>
      <div class="col-6 col-sm-4 ">
        <div class="card mx-auto" style="width: 11rem;">
          <!-- <h3 class="card-header">Today's Date</h3> -->

          <div class="card-body d-flex align-items-center">
            <img style="height: 200px; width: 100%; display: block;" src='<%=photoData[result].image_url%>'
              alt="Card image">
          </div>
          <div class="card-body text-center">
            <h5 class="card-text text-center"><%=photoData[result].name%></h5>
          </div>
        </div>
      </div>
      <% if (result % 3 == 2) { %>
        <div class="w-100 d-none d-md-block"></div>
    </div>
    <% } %>
    <% } %>

    <!-- </div> -->

    <!-- </div> -->
  </main>

  <footer class="footer">
    <% include /partials/footer %>
  </footer>

</body>
<script>
  $(document).ready(function () {
    var photoData = <%-JSON.stringify(photoData)%>


  })
</script>

</html>        